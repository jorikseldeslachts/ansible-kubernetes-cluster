# split masters in initial setup + additional HA masters
# Initial should only contain one master, all the rest belongs in the [master-ha] section

[masters:children]
master-initial
master-ha

[master-initial]
master-initial ansible_host=10.66.66.5 ansible_user=root

[master-ha]
master-ha-1 ansible_host=10.66.66.6 ansible_user=root
master-ha-2 ansible_host=10.66.66.7 ansible_user=root

[workers]
worker-1 ansible_host=10.66.66.8 ansible_user=root

